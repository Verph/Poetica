{
  "rules": [
    {
      "match": {
        "blocks": {
          "blocks": [
            "contenttweaker:mud_block:*"
          ]
        }
      },
      "replaceStrategy": "REPLACE_ITEMS_IF_SELECTED",
      "drops": [
        {
          "item": {
            "items": [
              "contenttweaker:mud_ball"
            ],
            "quantity": {
              "min": 2,
              "max": 4
            }
          }
        }
      ]
    }
  ]
}